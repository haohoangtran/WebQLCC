<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-title">
                <h4>Danh sách Admin</h4>
                <button class="btn btn-success" id="btaddAdmin" style="float: right">Thêm</button>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                        <tr>
                            <th>ID</th>

                            <th>Tên</th>
                            <th>Password</th>
                            <th>Ngày tạo</th>


                        </tr>
                        </thead>
                        <tbody>
                        {{#each admins}}
                            <tr>
                                <td><span>{{getIDIndex @index}}</span></td>

                                <td>{{username}}</td>
                                <td>{{password}}</td>
                                <td><span class="text-14 badge badge-success">{{dateCreate}}</span></td>
                            </tr>
                        {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>

<script>
    document.title = "Danh sách Administrator"

    $('#btaddAdmin').click(e => {
        window.location.href = '/addAdmin'
    })
</script>
